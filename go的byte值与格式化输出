package main
import"fmt"

func main() {

	var c1 byte = 'a'
	var c2 byte = 'o'//字符的o

	//当我们直接输出byte值，就是输出了的对应的字符的码值
	//'a'==>
	fmt.Println("c1=",c1)
	fmt.Println("c2=",c2)
//	//如果我们希望输出对应字符，需要使用格式化输出
	fmt.Printf("c1=%c c2=%c ",c1,c2)
//
//	//var c3 byte = 北 overflow溢出
//	var c3 byte ='北'//overflow溢出
//	fmt.Printf("c3=%c c3对应的码值=%",c3 ,c3)
//
//	var c4 int = 22269
//	fmt.Printf("c4=%c",c4)
	//字符类型是可以进行运算的，相当一个整数，运输时是按照码值运行
	var n1 = 10 + 'a'// 10 + 97 = 107
	fmt.Println("n1=", n1)
}
